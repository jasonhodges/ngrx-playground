{
  "compilerOptions": {
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "src",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "target": "es5",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2016",
      "dom"
    ],
    "suppressImplicitAnyIndexErrors": true,
    "paths": {
      "@ngrx-playground/*": [
        "app/*"
      ]
    }
  },
  "files": [
  ],
  "exclude": [
    "node_modules"
  ]
}
